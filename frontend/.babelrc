{
    "presets": [
    [
      "@babel/preset-env",
      {
        "targets": {
          "node": "current"
        },
        "useBuiltIns": "usage",
        "corejs": 3
      }
    ],
    "@babel/preset-typescript",
    "@babel/preset-react"
    ],
      "plugins": [
       "@babel/plugin-proposal-class-properties",
       "@babel/proposal-object-rest-spread",
       "@babel/plugin-syntax-dynamic-import"
    ]
}          
